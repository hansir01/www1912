/**
 * @param {String} fullName 专业全称
 * @param {Int} length 学制
 * @param {String} education 毕业学历 （大专、本科、双学历即中专+大专）
 * @param {Int} num 招生计划人数
 * @param {Int} vestigial 剩余名额
 * @param {Int} state 状态（1，正常招生、0,暂停招生，后期会重新招生、-1，已取消相关专业）
 * @param {Int} ent_num 企业班总名额
 * @param {Int} ent_vestigial 企业班剩余名额
 */

var sdxh_proList = {
    showList: function () {
        var list = new Array();
        // V1.2.0 Object.keys(sdxh_proList);
        for (var key in sdxh_proList)
            if (sdxh_proList[key]["state"] == 1) list.push(sdxh_proList[key]["fullName"]);
        return list;
    },

    showOptionList: function () {
        var list = this.showList();
        var OptionList = '';
        list.forEach(function (element) {
            //for...in不应该用于迭代Array，其索引顺序很重要。最好用for循环或Array.prototype.forEach()、for...of(for...of JS SE5 不支持)
            OptionList += "<option>" + element + "</option>";
        });
        return OptionList;
    },

